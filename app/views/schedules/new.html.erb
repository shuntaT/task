<h3>スケジュール新規作成</h3>
 
    <%= form_with model: @schedule do |f| %>
      <table>
        <tr>
          <th><%= f.label :title,"タイトル" %></th>
          <td><%= f.text_field :title %></td>
        </tr>
 
        <tr>
          <th><%= f.label :start,"開始日" %></th>
          <td><%= f.date_select :start %></td>
        </tr>
 
        <tr>
          <th><%= f.label :end,"終了日" %></th>
          <td><%= f.date_select :end %></td>
        </tr>
 
        <tr>
          <th><%= f.label :allday,"終日" %></th>
          <td><%= f.check_box :allday %></td>
        </tr>
      </table>
 
      <div>
        <ul>
          <li><%= f.submit "登録完了する"%></li>
          <li><%= link_to "スケジュール一覧に戻る", :schedules %></li>
        </ul>
      </div>
    <% end %>