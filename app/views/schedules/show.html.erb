 <h3>スケジュール詳細</h3>
 
    <table>
      <tr>
        <th>タイトル</th>
        <td><%= @schedule.title %></td>
      </tr>
      <tr>
        <th>開始日</th>
        <td><%= @schedule.start %></td>
      </tr>
      <tr>
        <th>終了日</th>
        <td><%= @schedule.end %></td>
      </tr>
      <tr>
        <th>終日</th>
        <td><% if @schedule.allday == true %> ○
        <% else @schedule.allday == false %>
        <% end %> </td>
      </tr>
    </table>
 
    <div>
      <ul>
        <li><%= link_to "スケジュール一覧に戻る", :schedules %></li>
      </ul>
    </div>